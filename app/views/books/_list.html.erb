<div class="col-md-3">
      <h2>User info</h2>
      <p><%= image_tag current_user.get_profile_image(100,100) %></p>
      <table class="table">
        <tbody>
          <tr></tr>
          <tr>
            <th>name</th>
            <th>matu</th>
          </tr>
          <tr>
            <th>introduktion</th>
            <th></th>
          </tr>
        </tbody>
      </table>
      <div class="row">
        <%= link_to edit_user_path(current_user.id),class:"btn btn-outline-secondary btn-block edit_user_80176" do %>
        <i class="fas fa-user-cog"></i>
        <% end %>
      </div>
      <h2 class=mt-3>New book</h2>
       <%= form_with model: book, url: '/books', method: :post do |f| %>

        <h4>Title</h4>
        <%= f.text_field :title %>

        <h4>Opinion</h4>
        <%= f.text_area :body %>

        <%= f.submit 'Create Book' %>

      <% end %>
      </div>